		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^23.4.2",
		"eslint": "^5.10.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-plugin-flowtype": "^3.2.0",
		"eslint-plugin-prettier": "^3.0.0",
		"flow-bin": "^0.89.0",
		"flow-copy-source": "^2.0.2",
		"jest": "^23.6.0",
